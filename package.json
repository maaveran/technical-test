{
  "name": "ts-loqiue",
  "version": "1.0.0",
  "description": "technical test",
  "main": "index.ts",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "start": "node dist/app.js",
    "test": "jest --config=jest.config.ts",
    "test:coverage": "jest --maxWorkers=20% --maxConcurrent=2 --verbose --detectOpenHandles --config=jest.config.ts --coverage",
    "dev": "nodemon src/app.ts",
    "seed:undo:all": "npx sequelize db:seed:undo:all",
    "seed:undo": "npx sequelize db:seed:undo --seed ",
    "seed:all": "npx sequelize db:seed:all",
    "seed": "npx sequelize db:seed --seed ",
    "migrate": "npx sequelize db:migrate ",
    "migrate:generate": "npx sequelize migration:generate --name",
    "seed:generate": "npx sequelize seed:generate --name"
  },
  "keywords": [
    "technical",
    "test",
    "interview"
  ],
  "author": "nailclipper <stephen.s.s.kom>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-fileupload": "^1.5.0",
    "express-winston": "^4.2.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "typescript": "^5.4.2",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21"
  }
}
